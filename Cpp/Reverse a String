#include <iostream>
#include <string>
using namespace std;

// Function to reverse a string
string reverseString(string str) {
    int n = str.length();
    for (int i = 0, j = n - 1; i < j; i++, j--) {
        swap(str[i], str[j]);
    }
    return str;  
}

int main() {
    string input;

    cout << "Enter a string: ";
    getline(cin, input);

    string reversed = reverseString(input);

    cout << "Reversed string: " << reversed << endl;

    return 0;
}
